               responsiveClass:true,
               responsive:{
                 0:{
                   items:2,
                   nav:false
                 },
                 600:{
                   items:3,
                   nav:false,
                   slideBy: 3
                 },
                 1000:{
                   items:5,
                   nav:false,
                   autoplayTimeout:4300,
                   loop:true,
                   slideBy: 5
                 }
               }
             });
            });
          </script>


